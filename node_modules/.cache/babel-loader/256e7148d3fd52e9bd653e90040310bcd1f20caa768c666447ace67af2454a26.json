{"ast":null,"code":"import{createSlice}from'@reduxjs/toolkit';const initialState={isMenuOpen:false,activeDropdown:null,isMobile:typeof window!=='undefined'?window.innerWidth<=768:false,isEnquiryModalOpen:false};const uiSlice=createSlice({name:'ui',initialState,reducers:{toggleMenu:state=>{state.isMenuOpen=!state.isMenuOpen;if(state.isMenuOpen){state.activeDropdown=null;}},closeMenu:state=>{state.isMenuOpen=false;state.activeDropdown=null;},setActiveDropdown:(state,action)=>{state.activeDropdown=action.payload;},setIsMobile:(state,action)=>{state.isMobile=action.payload;if(!action.payload){state.isMenuOpen=false;state.activeDropdown=null;}},openEnquiryModal:state=>{state.isEnquiryModalOpen=true;},closeEnquiryModal:state=>{state.isEnquiryModalOpen=false;}}});export const{toggleMenu,closeMenu,setActiveDropdown,setIsMobile,openEnquiryModal,closeEnquiryModal}=uiSlice.actions;export default uiSlice.reducer;","map":null,"metadata":{},"sourceType":"module","externalDependencies":[]}